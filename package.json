{
  "name": "voice-of-upsa",
  "version": "1.0.0",
  "description": "A modern news website for UPSA built with Next.js, TypeScript, Supabase authentication, Supabase database, and TailwindCSS.",
  "main": "next.config.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:clean": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "clean": "powershell -Command \"if (Test-Path out) { Remove-Item -Recurse -Force out }; if (Test-Path .next) { Remove-Item -Recurse -Force .next }; if (Test-Path tsconfig.tsbuildinfo) { Remove-Item tsconfig.tsbuildinfo }\"",
    "clean:full": "npm run clean && powershell -Command \"if (Test-Path node_modules) { Remove-Item -Recurse -Force node_modules }; if (Test-Path package-lock.json) { Remove-Item package-lock.json }\" && npm install",
    "validate-db": "node scripts/validate-database.js",
    "setup-db": "node scripts/run-setup.js",
    "reset-clean": "node scripts/reset-clean.js",
    "supabase": "supabase"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.38.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "cloudinary": "^1.41.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "formidable": "^3.5.1",
    "framer-motion": "^10.16.0",
    "next": "^14.0.0",
    "next-cloudinary": "^5.0.0",
    "next-themes": "^0.2.0",
    "nodemailer": "^6.10.1",
    "postcss": "^8.4.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.68.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.11.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.5.1",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.5",
    "styled-components": "^6.1.19",
    "svix": "^1.81.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/d3-color": "^3.1.3",
    "@types/d3-ease": "^3.0.2",
    "@types/d3-path": "^3.1.1",
    "@types/d3-time": "^3.0.4",
    "@types/d3-timer": "^3.0.2",
    "@types/formidable": "^3.4.6",
    "@types/nodemailer": "^6.4.21",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.0.0",
    "supabase": "^2.58.5",
    "ts-node": "^10.9.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
