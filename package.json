{
  "name": "voice-of-upsa",
  "version": "1.0.0",
  "type": "module",
  "description": "A modern news website for UPSA built with Next.js, TypeScript, Supabase authentication, Supabase database, and TailwindCSS.",
  "main": "next.config.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "dev": "next dev",
    "dev:clean": "npm run clean && next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:clean": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "security-check": "node scripts/security-health-check.cjs",
    "type-check": "tsc --noEmit --skipLibCheck",
    "clean": "powershell -Command \"if (Test-Path out) { Remove-Item -Recurse -Force out }; if (Test-Path .next) { Remove-Item -Recurse -Force .next }; if (Test-Path tsconfig.tsbuildinfo) { Remove-Item tsconfig.tsbuildinfo }\"",
    "clean:full": "npm run clean && powershell -Command \"if (Test-Path node_modules) { Remove-Item -Recurse -Force node_modules }; if (Test-Path package-lock.json) { Remove-Item package-lock.json }\" && npm install",
    "validate-db": "node scripts/validate-database.js",
    "setup-db": "node scripts/run-setup.js",
    "reset-clean": "node scripts/reset-clean.js",
    "supabase": "supabase"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "@types/dompurify": "^3.2.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.27",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.36.0",
    "@vercel/speed-insights": "^1.3.1",
    "autoprefixer": "^10.4.23",
    "axios": "^1.6.0",
    "cloudinary": "^2.8.0",
    "clsx": "^2.1.1",
    "csrf": "^3.1.0",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "file-type": "^21.2.0",
    "formidable": "^3.5.4",
    "framer-motion": "^11.0.0",
    "isomorphic-dompurify": "^2.35.0",
    "jsonwebtoken": "^9.0.3",
    "next": "^16.0.8",
    "next-cloudinary": "^6.0.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.12",
    "pg": "^8.16.3",
    "postcss": "^8.4.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.11.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.6.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.5",
    "svix": "^1.84.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.0.0",
    "validator": "^13.15.26",
    "xss": "^1.0.15",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/d3-color": "^3.1.3",
    "@types/d3-ease": "^3.0.2",
    "@types/d3-path": "^3.1.1",
    "@types/d3-time": "^3.0.4",
    "@types/d3-timer": "^3.0.2",
    "@types/formidable": "^3.4.6",
    "@types/nodemailer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.0.0",
    "supabase": "^2.58.5",
    "ts-node": "^10.9.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
